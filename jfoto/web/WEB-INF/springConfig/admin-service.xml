<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	   default-autowire="byName">

	<bean id="upgradeService" class="admin.services.services.UpgradeServiceImpl"/>
	<bean id="upgradeTaskUtilsService" class="admin.services.services.SqlUtilsServiceImpl"/>

	<bean id="jobExecutionService" class="admin.services.jobs.JobExecutionServiceImpl"/>
	<bean id="jobStatusChangeStrategyService" class="admin.services.jobs.JobStatusChangeStrategyServiceImpl" />
	<bean id="jobHelperService" class="admin.services.jobs.JobHelperServiceImpl" />
	<bean id="jobExecutionHistoryService" class="admin.services.jobs.JobExecutionHistoryServiceImpl"/>
	<bean id="savedJobService" class="admin.services.jobs.SavedJobServiceImpl" />

	<bean name="upgradeDao" class="admin.services.services.UpgradeDaoImpl"/>
	<bean id="jobExecutionHistoryDao" class="admin.services.jobs.JobExecutionHistoryDaoImpl"/>
</beans>