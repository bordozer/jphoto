</head>
<body class="base-font-color">

	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-2">
				<div>
					<a href="$portalpageurl">$progectname</a>
				</div>
				<div>
					<a href="$portalpageurl">$slogan</a>
				</div>
				<div>
					#foreach( $uiLanguage in $uiLanguages )
						<a href="#" onclick="changeUiLanguage( '$uiLanguage.language.code' ); return false;">
							<img src="$imagePath/languages/$uiLanguage.language.icon" title="$uiLanguage.title" class="language $uiLanguage.style">
						</a>
					#end

					<script type="text/javascript">
						function changeUiLanguage( languageCode ) {
							$.ajax( {
										type:'GET',
										url:'$baseUrl/language/' + languageCode + '/',
										success:function ( response ) {
											document.location.reload();
										},
										error:function () {
											alert( 'Error changing UI language :' );
										}
									} );
						}
					</script>
				</div>
			</div>
			<div class="col-lg-8">
				<div>$pageHeader</div>
				<div>$breadcrumbs</div>
				<div>
					#foreach( $titleIcon in $titleIcons )
						<div class="col-md-1">$titleIcon.icon</div>
					#end

					#if ( $hiMessage )
						<script type="text/javascript">
							require( [ 'jquery' ], function( $ ) {
								$( document ).ready( function() {
									showUIMessage_Notification( "$hiMessage" );
								});
							});
						</script>
					#end
				</div>
			</div>
			<div class="col-lg-2">
				#parse( "loginForm.vm" )
			</div>
		</div>
	</div>

	<div>
		#parse( "menu.vm" )
	</div>

	#if ( $showPhotoCategoriesToolbar )
		<div>
			#parse( "genrelist.vm" )
		</div>
	#end

